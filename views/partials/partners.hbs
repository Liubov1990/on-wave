<section class="section partners">
  <h2 class="section__heading partners__heading">Our partners</h2>
    <div class="section-slider section-slider__container partners-slider__container">
    <button class="section-slider__prev-arrow partners-slider__prev-arrow">
      <svg class="section-slider__prev-icon fill-arrow-color">
        <use xlink:href="./assets/svg/sprite.svg#prev" />
      </svg>
    </button>
    <div class="partners-slider">
      <ul class="partners-slider__list">
        {{#each data}}
          {{>partner data=data}}
        {{/each}}
      </ul>
    </div>
    <button class="section-slider__next-arrow partners-slider__next-arrow">
      <svg class="section-slider__next-icon fill-arrow-color">
        <use xlink:href="./assets/svg/sprite.svg#next" />
      </svg>
    </button>
  </div>
</section>
